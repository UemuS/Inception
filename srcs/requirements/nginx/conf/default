server{

	listen 443 ssl ;
	listen [::]:443 ssl ;
	server_name _;
	ssl_protocols TLSv1.3;
	ssl_certificate /etc/ssl/certs/yihssan.crt;
	ssl_certificate_key /etc/ssl/private/yihssan.key;

	root /var/www/html;
	index index.html index.htm index.nginx-debian.html index.php;

	location / {
		autoindex on;
		try_files $uri $uri/ =404;
	}
}